pipeline
{
 agent any
 stages
  {
     stage('git clone')
     { steps
       {sh 'echo download '}
     }
  
    stage('code compile')
    {steps
      { 
        sh 'echo code is compiling' 
      }
    }
    stage('approval')
    {steps
      { input  "please approve deployment?"}
    }
    
    stage('deployment')
    {steps
      { 
       sh 'echo deployment' 
      }
    }
    
 }   
  } //Stages
 }/Pipeline
    
    
