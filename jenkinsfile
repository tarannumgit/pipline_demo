pipeline
{
 agent any
 stages
  {
     stage('git clone')
     { steps
       {sh 'echo download '}
     }
  
    stage('code compile')
    {steps
      {   sh 'echo code is compiling'   }
    }
   
    stage('get approval')
    {steps
    {input "please enter input?" }
    }
    
    stage(''deployment)
    {steps
     {sh 'deployment' }
     }



  } //Stages
 }/Pipeline
    
    
